{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [ "error", {
            "code": 80,
            "tabWidth": 2,
            "ignoreUrls": true,
            "ignoreComments": true
          }]
    }
}
